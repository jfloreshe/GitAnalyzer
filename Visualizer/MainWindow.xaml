<Window x:Class="Visualizer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Visualizer"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="500">
    

        <Grid >
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>

        <TextBlock x:Name="factoryData" Grid.Column="0"></TextBlock>
        <StackPanel Grid.Column="1" Orientation="Vertical" VerticalAlignment="Center">
            <Button x:Name="getData" Padding="20 10" Click="getData_click" Content="Get Data"/>
            <Button x:Name="openChildForm" Padding="20 10" Click="openChildForm_Click" Content="Open Child Form"/>
            <Button x:Name="openChildForm2" Padding="20 10" Click="openChildForm2_Click" Content="Open Child Form 2"/>
        </StackPanel>
        <TextBlock x:Name="normalData" Grid.Column="3"></TextBlock>
    </Grid>
            
</Window>
